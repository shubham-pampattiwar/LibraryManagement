<%if @librarians1.first %>
<table>
  <thead>
  <tr>
    <th colspan="3">Librarian Name</th>
  </tr>
  </thead>
  <tbody>
  <% @librarians1.each do |lib| %>
      <tr>
        <td colspan="3"> <%= lib.name %> </td>
        <td colspan="3"> <%= link_to 'Approve', approve_librarian_path(:id => lib.id) %> </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%else %>
    <p>There are no approval requests currently</p>
<% end %>
<%=link_to "Back to dashboard", admins_path %>