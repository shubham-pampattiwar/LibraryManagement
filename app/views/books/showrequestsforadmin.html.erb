<h2>All hold requests </h2>
<table>
  <thead>
  <tr>
    <th colspan="3">Library Name</th>
    <th colspan="3">Student Name</th>
    <th colspan="3">Email ID</th>
    <th colspan="3">Education Level</th>
    <th colspan="3">Book Name</th>
    <th colspan="3">Queue Number</th>
  </tr>
  </thead>
  <tbody>
  <% @req.each do |requests| %>
    <% requests.each do |request| %>
      <tr>
        <td colspan="3"> <%= Library.find(Book.find(request.book_id).library_id).name %> </td>
        <td colspan="3"> <%= Student.find(request.student_id).name %> </td>
        <td colspan="3"> <%= Student.find(request.student_id).email %> </td>
        <td colspan="3"> <%= Student.find(request.student_id).education_level %> </td>
        <td colspan="3"> <%= Book.find(request.book_id).title %> </td>
        <td colspan="3"> <%= request.queuenumber %> </td>
      </tr>

    <% end %>
  <% end %>
  </tbody>
</table>
<%=link_to "Back to dashboard", admins_path %>