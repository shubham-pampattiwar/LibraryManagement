<h1>Books issued and Overdue fine(s):</h1>
<table>
<thead>
   <tr>
     <th>Title</th>
     <th>Overdue Fine($)</th>
     <th colspan="3"></th>
   </tr>
 </thead>
  <tbody>
            <% @overduefines.each do |hash| %>
        <tr>
            <td><%= Book.where('id = ?',hash[:book_id]).first.title %></td>
            <td><%= hash[:fine_value] %></td>           
        </tr>
        
    <% end %>
  </tbody>
</table>